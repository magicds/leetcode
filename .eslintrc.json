{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    // 使用默认的配置
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "no-delete-var": "warn",
        "no-extra-semi": "warn",
        // 单引号警告
        "quotes": ["warn", "single"],
        // 强制所有控制语句使用一致的括号风格
        // "curly": "warn",
        // 要求 switch 语句中有 default 分支
        "default-case": "warn",

        "no-multiple-empty-lines": "warn",
        "no-console": "off",
        "no-empty": "off"
    }
}